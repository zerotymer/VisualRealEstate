<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:views="clr-namespace:ModularApp.Modules.Views"
             xmlns:viewModels="clr-namespace:ModularApp.Modules.ViewModels"
             xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
             xmlns:dxm="http://schemas.devexpress.com/winfx/2008/xaml/map" x:Class="ModularApp.Modules.Views.DevMapView"
             mc:Ignorable="d" d:DesignHeight="300" d:DesignWidth="600"
             d:DataContext="{dxmvvm:ViewModelSource viewModels:DevMapViewModel}">
    <dxm:MapControl>
        <dxm:ImageLayer>
            <dxm:OpenStreetMapDataProvider Kind="Basic"/>
        </dxm:ImageLayer>
    </dxm:MapControl>
</UserControl>
